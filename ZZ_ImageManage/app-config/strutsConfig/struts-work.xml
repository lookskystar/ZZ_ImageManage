<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="work" extends="common" namespace="/">
		<action name="loginAction" class="loginAction">
	  		<result name="main" type="redirect">/main.jsp</result>
	  		<result name="loginOut" type="redirect">login.jsp</result>
	  	</action>
	  	
	  	<action name="function" class="functionAction">
	  		<result name="left">left.jsp</result>
	  	</action>
	  	
	  	<action name="deviceAction" class="deviceAction">
	  		<result name="device">work/device/device.jsp</result>
	  		<result name="adddevice">work/device/adddevice.jsp</result>
	  		<result name="deviceInfo">work/device/deviceInfo.jsp</result>
	  		<result name="receivedevice">work/device/receivedevice.jsp</result>
	  		<result name="returndevice">work/device/returndevice.jsp</result>
	  		<result name="resume">work/device/resume.jsp</result>
	  	</action>
	  	
	  	<action name="countJcAction" class="countJcAction">
	  		<result name="countJc">CountJc.jsp</result>
	  	</action>
	  	
	  	<action name="workAction" class="workAction">
	  		<result name="work_input">work/work_input.jsp</result>
	  	</action>
	  	
	  	<action name="ftpValidate" class="ftpValidateAction"></action>
	</package>
</struts>
